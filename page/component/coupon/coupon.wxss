@import '../../common/common.wxss';

.banner image{
    width: 100%;
    height: 400rpx;
}
.list-box{
    padding: 30rpx 20rpx;
}
.newest-list{
    display: inline-block;
    width: 347rpx;
    height: 400rpx;
    margin:0 15rpx 10rpx 0;
    border-radius: 10px;
    text-align: center;
    background: #f5f6f5;
    font-family: cursive;
    font-weight: bold;
}

.fontStyle{
  font-size: 12px;
}

.newest-list:nth-child(2n){
    margin-right: 0;
}
.newest-list image{
    width: 180rpx;
    height: 130rpx;
    margin: 30rpx 0 20rpx;
}
.newest-list .newest-text{
    font-size: 32rpx;
    line-height: 45rpx;
}

.content-btn-group{
  padding: 10px;
}

/**默认按钮点击 高亮样式**/
.button-hover {
  background-color: red;
  color:#fff;
}
/** 添加自定义button点击态样式类**/
.other-button-hover {
  background-color: blue;
}
.btn{
  margin-top: 10px;
}
